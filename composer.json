{
    "name": "subugoe/counter-client",
    "description": "",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "http://openapi-generator.tech",
    "license": "proprietary",
    "authors": [
        {
            "name": "OpenAPI-Generator contributors",
            "homepage": "https://openapi-generator.tech"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.9"
    },

    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "squizlabs/php_codesniffer": "^3.10",
        "friendsofphp/php-cs-fixer": "^3.59",
        "rector/rector": "^1.0"
    },

    "autoload": {
        "psr-4": {
            "Subugoe\\CounterClient\\": "lib/"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Subugoe\\CounterClient\\Test\\": "test/"
        }
    },
    "scripts": {
      "cleanup": [
        "rector process",
        "php-cs-fixer fix"
      ],
      "lint": [
        "php-cs-fixer fix --dry-run --diff",
        "rector process --dry-run",
        "phpstan analyze src"
      ]
    }
}
